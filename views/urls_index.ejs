<head>

  <title>TinyApp</title>
</head>
<body >
  <header>
      <% include ./partials/_header %>
  </header>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">


  <ul>
    My URLs
    <h5><a href='/urls/new' >Create New TinyURL</a></h5>
    <% for(let short in urls) { %>
    <li><%= short %> : <a href='#' ><%= urls[short].longURL %></a>
      <div style="display: flex;">
      <form method="POST" action="/urls/<%= short %>/edit">
        <button type="submit" class="btn btn-primary" style="margin-right: 15px;">Edit</button>
      </form>
      <form method="POST" action="/urls/<%= short %>/delete">
        <button type="submit" class="btn btn-primary">Delete</button>
      </form>
    </div>
      </li>
    <% } %>
  </ul>
</body>

